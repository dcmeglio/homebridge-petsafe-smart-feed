{
  "name": "homebridge-petsafe-smart-feed",
  "version": "1.0.0",
  "description": "Homebridge plugin for PetSafe Smart Feed",
  "main": "lib/index.js",
  "bin": {
    "petsafe-auth-cli": "petsafe-auth-cli.js"
  },
  "scripts": {
    "test": "eslint **/*.ts",
    "lint": "eslint **/*.ts --fix",
    "build": "rm -rf lib && tsc",
    "example": "ts-node ./examples/example.ts",
    "auth-cli": "npm run build && ./petsafe-auth-cli.js",
    "test-homebridge": "npm run build && ./build/package.sh && homebridge -P ./release",
    "prepare-npm-package": "./build/package.sh",
    "push-with-tags": "git push --follow-tags && node ./build/github-releaser.js",
    "deploy": "standard-version && npm publish release && npm run push-with-tags",
    "deploy-alpha": "standard-version --prerelease alpha && npm publish --tag alpha release && npm run push-with-tags"
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run build && npm test",
      "posttag": "npm run prepare-npm-package"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dgreif/homebridge-petsafe-smart-feed.git"
  },
  "keywords": [
    "homebridge-plugin",
    "petsafe",
    "smart feed",
    "feeder",
    "pet"
  ],
  "author": "dgreif",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.21",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "chai": "^4.2.0",
    "conventional-github-releaser": "^3.1.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "homebridge": "^0.4.50",
    "homebridge-config-ui-x": "^4.6.6",
    "mocha": "^6.2.2",
    "prettier": "^1.19.1",
    "standard-version": "^7.0.1",
    "ts-essentials": "^4.0.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "engines": {
    "node": ">=10",
    "homebridge": ">=0.4.0"
  },
  "bugs": {
    "url": "https://github.com/dgreif/homebridge-petsafe-smart-feed/issues"
  },
  "homepage": "https://github.com/dgreif/homebridge-petsafe-smart-feed#readme"
}
